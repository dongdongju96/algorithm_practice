   "source": [
    "N, K =  map(int, input().split())\n",
    "divisor = []\n",
    "\n",
    "for div in range(1,N+1):\n",
    "    if N%div==0:\n",
    "        divisor.append(div)\n",
    "        \n",
    "if len(divisor) < K:\n",
    "    print(0)\n",
    "else:\n",
    "    print(divisor[K-1])"
   ]
